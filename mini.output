Gramática

    0 $accept: programa $end

    1 programa: definicion_programa

    2 definicion_programa: PROGRAMA nombre ';' bloque_programa
    3                    | error ';'

    4 nombres: nombre
    5        | nombres ',' nombre

    6 nombre: IDENTIFICADOR

    7 bloque_programa: declaracion_tipos declaracion_constantes declaracion_variables bloque_instrucciones

    8 declaracion_tipos: %empty
    9                  | TIPOS declaraciones_tipos

   10 declaraciones_tipos: declaracion_tipo
   11                    | declaraciones_tipos declaracion_tipo

   12 declaracion_tipo: nombre '=' tipo_no_estructurado_o_nombre_tipo ';'
   13                 | error ';'

   14 tipo_escalar: ENTERO
   15             | REAL
   16             | CARACTER
   17             | CADENA
   18             | error ';'

   19 tipo_fichero: FICHERO

   20 tipo_enumerado: '(' expresiones_constantes ')'

   21 tipo_lista: ARRAY DE tipo_no_estructurado_o_nombre_tipo
   22           | ARRAY '[' rangos ']' DE tipo_no_estructurado_o_nombre_tipo

   23 tipo_conjunto: CONJUNTO DE tipo_no_estructurado_o_nombre_tipo

   24 tipo_no_estructurado_o_nombre_tipo: nombre
   25                                   | tipo_escalar
   26                                   | tipo_fichero
   27                                   | tipo_enumerado
   28                                   | tipo_lista
   29                                   | tipo_conjunto

   30 expresiones_constantes: expresion_constante
   31                       | expresiones_constantes ',' expresion_constante
   32                       | error ';'

   33 rangos: rango
   34       | rangos ',' rango
   35       | error ';'

   36 rango: expresion ':' expresion
   37      | expresion ':' expresion ':' expresion

   38 declaracion_constantes: %empty
   39                       | CONSTANTES declaraciones_constantes

   40 declaraciones_constantes: declaracion_constante
   41                         | declaraciones_constantes declaracion_constante

   42 declaracion_constante: nombre ':' tipo_no_estructurado_o_nombre_tipo '=' valor_constante ';'
   43                      | error ';'

   44 valor_constante: expresion
   45                | '[' valores_constantes ']'

   46 valores_constantes: valor_constante
   47                   | valores_constantes ',' valor_constante

   48 declaracion_variables: %empty
   49                      | VARIABLES declaraciones_variables

   50 declaraciones_variables: declaracion_variable
   51                        | declaraciones_variables declaracion_variable
   52                        | error ';'

   53 declaracion_variable: nombres ':' tipo_no_estructurado_o_nombre_tipo ';'
   54                     | nombres ':' tipo_no_estructurado_o_nombre_tipo '=' valor_constante ';'

   55 bloque_instrucciones: '{' bloques_instrucciones '}'

   56 bloques_instrucciones: instruccion
   57                      | bloques_instrucciones instruccion

   58 instruccion: ';'
   59            | instruccion_asignacion
   60            | instruccion_salir
   61            | instruccion_devolver
   62            | instruccion_si
   63            | instruccion_bucle
   64            | instruccion_lanzar
   65            | error ';'

   66 instruccion_asignacion: objeto '=' expresion ';'

   67 instruccion_salir: SALTAR ';'
   68                  | SALTAR SI expresion ';'

   69 instruccion_devolver: DEVOLVER ';'
   70                     | DEVOLVER expresion ';'

   71 instruccion_si: SI expresion SINO bloque_instrucciones
   72               | SI expresion SINO bloque_instrucciones SINO bloque_instrucciones

   73 instruccion_bucle: clausula_iteracion bloque_instrucciones

   74 clausula_iteracion: PARA nombre
   75                   | MIENTRAS CONJUNTO nombre
   76                   | MIENTRAS expresion
   77                   | MIENTRAS FIN expresion
   78                   | error ';'

   79 instruccion_lanzar: EJECUTA nombre ';'

   80 expres: expresion
   81       | expres ',' expresion

   82 expresion: expresion1

   83 expresion1: expresion1 OR expresion2
   84           | expresion2

   85 expresion2: expresion2 AND expresionNegacion
   86           | expresionNegacion

   87 expresionNegacion: '!' expresion3
   88                  | expresion3

   89 expresion3: expresion3 LE expresion4
   90           | expresion3 GE expresion4
   91           | expresion3 '<' expresion4
   92           | expresion3 '>' expresion4
   93           | expresion3 EQ expresion4
   94           | expresion3 NEQ expresion4
   95           | expresion4

   96 expresion4: expresion4 '^' expresion5
   97           | expresion5

   98 expresion5: expresion5 '@' expresion6
   99           | expresion6

  100 expresion6: expresion6 '&' expresion7
  101           | expresion7

  102 expresion7: expresion7 FLECHA_IZDA expresion8
  103           | expresion7 FLECHA_DCHA expresion8
  104           | expresion8

  105 expresion8: expresion8 '+' expresion9
  106           | expresion8 '-' expresion9
  107           | expresion9

  108 expresion9: expresion9 '*' expresion10
  109           | expresion9 '/' expresion10
  110           | expresion9 '%' expresion10
  111           | expresion10

  112 expresion10: expresion11 POTENCIA expresion10
  113            | expresion11

  114 expresion11: "-" expresion_primaria
  115            | expresion_primaria

  116 expresion_primaria: expresion_constante
  117                   | objeto
  118                   | '(' expresion ')'
  119                   | error ';'

  120 objeto: nombre
  121       | objeto '[' expres ']'
  122       | objeto '.' IDENTIFICADOR

  123 expresion_constante: CTC_ENTERA
  124                    | CTC_REAL
  125                    | CTC_CADENA
  126                    | CTC_CARACTER


